<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="trendoceanJTA" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>com.trendocean.domain.Profile</class>
       	<class>com.trendocean.domain.Country</class>
        <class>com.trendocean.domain.City</class>
        <class>com.trendocean.domain.Message</class>
        <class>com.trendocean.domain.VisibilityEnum</class>

        <class>com.trendocean.domain.Question</class>
    </persistence-unit>




</persistence>